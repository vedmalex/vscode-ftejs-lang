{
    "name": "fte-js-lang",
    "displayName": "fte.js-lang",
    "description": "syntax highlight for fte.js templates",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "fte",
                "aliases": [
                    "fte",
                    "fte"
                ],
                "extensions": [
                    "njs",
                    "n.js",
                    "njst",
                    "nhtml",
                    "n.html"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "fte",
                "scopeName": "text.fte",
                "path": "./syntaxes/fte.tmLanguage.json"
            }
        ]
    }
}